{
  "name": "test-nest-auth-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/test-nest-auth-lib/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "{projectRoot}/dist",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist",
        "tsConfig": "libs/test-nest-auth-lib/tsconfig.lib.json",
        "packageJson": "libs/test-nest-auth-lib/package.json",
        "main": "libs/test-nest-auth-lib/src/index.ts"
      }
    },
    "nx-release-publish": {
      "dependsOn": ["build"],
      "options": {
        "packageRoot": "{projectRoot}/dist"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/test-nest-auth-lib/jest.config.ts"
      }
    }
  }
}
