{
  "name": "say-hello-cross",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/say-hello-cross/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/say-hello-cross",
        "main": "libs/say-hello-cross/src/index.ts",
        "tsConfig": "libs/say-hello-cross/tsconfig.lib.json",
        "assets": [
          {
            "input": "libs/say-hello-cross",
            "glob": "*.md",
            "output": "./"
          }
        ],
        "project": "libs/say-hello-cross/package.json",
        "compiler": "swc",
        "format": ["cjs", "esm"]
      }
    },
    "nx-release-publish": {
      "dependsOn": ["build"],
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/libs/say-hello-cross"
      }
    }
  }
}
