{
  "name": "its-battistar-nest",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/its-battistar-nest/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "compose": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "docker compose -f ./docker-compose.yml  up --no-recreate -d"
        ],
        "cwd": "apps/its-battistar-nest"
      }
    },
    "serve.bak": {
      "dependsOn": ["compose"],
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "its-battistar-nest:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "its-battistar-nest:build:development"
        },
        "production": {
          "buildTarget": "its-battistar-nest:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/its-battistar-nest/jest.config.ts"
      }
    }
  }
}
